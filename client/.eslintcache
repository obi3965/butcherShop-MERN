Name=\"card-block text-xs-center\">\r\n            <p className=\"h4 card-title font-weight-bold\"> {name} </p>\r\n            <p className=\"h6 card-subtitle text-muted\"> {email} </p><br />\r\n            <p className=\"h6 card-subtitle text-muted\"> {role === 1 ? \"Admin\" : \"Registered user\"} </p><br />\r\n          </div>\r\n        </div>\r\n      \r\n       )\r\n   }\r\n\r\n   const purchaseHistory = () =>{\r\n       return (\r\n           <div className=\"card mb-5\">\r\n               <h3 className=\"card-header\">purchase history</h3>\r\n               <ul className=\"list-group\">\r\n                   <li className=\"list-group-item\">history</li>\r\n               </ul>\r\n           </div>\r\n       )\r\n   }\r\n  return(\r\n    <Layout title=\"Dashboard\" description={`G'day ${name}`}>\r\n       <div className=\"container\">\r\n       <div className=\"row py-3 flex-items-sm-center\">\r\n     <div className=\"col-lg-4 col-xs-12 col-sm-3 py-2 clearfix\">\r\n       {userLinks()}\r\n     </div>\r\n         <div className=\"col-lg-8 col-xs-12 col-sm-3 py-2 clearfix\">\r\n         {userInfo()}\r\n         {purchaseHistory()}\r\n     </div>\r\n      </div>\r\n       </div>\r\n    </Layout>\r\n   )\r\n\r\n }\r\n\r\nexport default Dashboard\r\n","C:\\Users\\Administrator\\Desktop\\slagterApp\\butcherShop-MERN\\client\\src\\auth\\PrivateRoute.js",[],"C:\\Users\\Administrator\\Desktop\\slagterApp\\butcherShop-MERN\\client\\src\\components\\ProductLayout.js",[],"C:\\Users\\Administrator\\Desktop\\slagterApp\\butcherShop-MERN\\client\\src\\auth\\AdminRoute.js",[],"C:\\Users\\Administrator\\Desktop\\slagterApp\\butcherShop-MERN\\client\\src\\user\\AdminDashboard.js",["127"],"import React from 'react'\r\nimport Layout from '../core/Layout'\r\nimport '../css/dashboard.css'\r\nimport { isAuthenticated } from '../auth'\r\nimport { Link } from 'react-router-dom'\r\n/**\r\n* @author\r\n* @function AdminDashboard\r\n**/\r\n\r\nconst AdminDashboard = (props) => {\r\n\r\n    const {user: {_id, name, email, role}} = isAuthenticated()\r\n\r\n    const adminLinks = () =>{\r\n        return (\r\n            <div className=\"card\">\r\n                <h4 className=\"card-header\">Admin links</h4>\r\n                 <ul className=\"list-group\">\r\n                     <li className=\"list-group-item\">\r\n                         <Link className=\"nav-link\" to=\"/category/create\">create category</Link>\r\n                     </li>\r\n                     <li className=\"list-group-item\">\r\n                     <Link className=\"nav-link\" to=\"/product/create\">create product</Link>\r\n                     </li>\r\n                 </ul>\r\n            </div>\r\n        )\r\n    }\r\n\r\n   const adminInfo = () =>{\r\n       return (\r\n       \r\n        <div className=\"card profile-card\">\r\n          <figure>\r\n            <input id=\"fab\" type=\"checkbox\" className=\"fab\" />\r\n            <label htmlFor=\"fab\" className=\"toggle\">+</label>\r\n             \r\n          </figure>\r\n          <div className=\"card-block text-xs-center\">\r\n            <p className=\"h4 card-title font-weight-bold\"> {name} </p>\r\n            <p className=\"h6 card-subtitle text-muted\"> {email} </p><br />\r\n            <p className=\"h6 card-subtitle text-muted\"> {role === 1 ? \"Admin\" : \"Registered user\"} </p><br />\r\n          </div>\r\n        </div>\r\n      \r\n       )\r\n   }\r\n\r\n   \r\n  return(\r\n    <Layout title=\"Dashboard\" description={`G'day ${name}`}>\r\n       <div className=\"container\">\r\n       <div className=\"row py-3 flex-items-sm-center\">\r\n     <div className=\"col-lg-4 col-xs-12 col-sm-3 py-2 clearfix\">\r\n       {adminLinks()}\r\n     </div>\r\n         <div className=\"col-lg-8 col-xs-12 col-sm-3 py-2 clearfix\">\r\n         {adminInfo()}\r\n        \r\n     </div>\r\n      </div>\r\n       </div>\r\n    </Layout>\r\n   )\r\n\r\n }\r\n\r\nexport default AdminDashboard\r\n","C:\\Users\\Administrator\\Desktop\\slagterApp\\butcherShop-MERN\\client\\src\\admin\\AddCategory.js",[],"C:\\Users\\Administrator\\Desktop\\slagterApp\\butcherShop-MERN\\client\\src\\admin\\AdminApi.js",[],"C:\\Users\\Administrator\\Desktop\\slagterApp\\butcherShop-MERN\\client\\src\\admin\\AddProduct.js",["128","129","130","131","132"],"import React, { useState, useEffect } from 'react';\r\nimport Layout from '../core/Layout';\r\nimport { isAuthenticated } from '../auth';\r\nimport { Link } from 'react-router-dom';\r\nimport { createProduct, getCategories } from './AdminApi';\r\n\r\nconst AddProduct = () => {\r\n    const [values, setValues] = useState({\r\n        name: '',\r\n        description: '',\r\n        price: '',\r\n        categories: [],\r\n        category: '',\r\n        shipping: '',\r\n        quantity: '',\r\n        photo: '',\r\n        loading: false,\r\n        error: '',\r\n        createdProduct: '',\r\n        redirectToProfile: false,\r\n        formData: ''\r\n    });\r\n\r\n    const { user, token } = isAuthenticated();\r\n    const {\r\n        name,\r\n        description,\r\n        price,\r\n        categories,\r\n        category,\r\n        shipping,\r\n        quantity,\r\n        loading,\r\n        error,\r\n        createdProduct,\r\n        redirectToProfile,\r\n        formData\r\n    } = values;\r\n\r\n    // load categories and set form data\r\n    const init = () => {\r\n        getCategories().then(data => {\r\n            if (data.error) {\r\n                setValues({ ...values, error: data.error });\r\n            } else {\r\n                setValues({\r\n                    ...values,\r\n                    categories: data,\r\n                    formData: new FormData()\r\n                });\r\n            }\r\n        });\r\n    };\r\n\r\n    useEffect(() => {\r\n        init();\r\n    }, []);\r\n\r\n    const handleChange = name => event => {\r\n        const value = name === 'photo' ? event.target.files[0] : event.target.value;\r\n        formData.set(name, value);\r\n        setValues({ ...values, [name]: value });\r\n    };\r\n\r\n    const clickSubmit = event => {\r\n        event.preventDefault();\r\n        setValues({ ...values, error: '', loading: true });\r\n\r\n        createProduct(user._id, token, formData).then(data => {\r\n            if (data.error) {\r\n                setValues({ ...values, error: data.error });\r\n            } else {\r\n                setValues({\r\n                    ...values,\r\n                    name: '',\r\n                    description: '',\r\n                    photo: '',\r\n                    price: '',\r\n                    quantity: '',\r\n                    loading: false,\r\n                    createdProduct: data.name\r\n                });\r\n            }\r\n        });\r\n    };\r\n\r\n    const newPostForm = () => (\r\n        <form className=\"mb-3\" onSubmit={clickSubmit}>\r\n            <h4>Post Photo</h4>\r\n            <div className=\"form-group\">\r\n                <label className=\"btn btn-secondary\">\r\n                    <input onChange={handleChange('photo')} type=\"file\" name=\"photo\" accept=\"image/*\" />\r\n                </label>\r\n            </div>\r\n\r\n            <div className=\"form-group\">\r\n                <label className=\"text-muted\">Name</label>\r\n                <input onChange={handleChange('name')} type=\"text\" className=\"form-control\" value={name} />\r\n            </div>\r\n\r\n            <div className=\"form-group\">\r\n                <label className=\"text-muted\">Description</label>\r\n                <textarea onChange={handleChange('description')} className=\"form-control\" value={description} />\r\n            </div>\r\n\r\n            <div className=\"form-group\">\r\n                <label className=\"text-muted\">Price</label>\r\n                <input onChange={handleChange('price')} type=\"number\" className=\"form-control\" value={price} />\r\n            </div>\r\n\r\n            <div className=\"form-group\">\r\n                <label className=\"text-muted\">Category</label>\r\n                <select onChange={handleChange('category')} className=\"form-control\">\r\n                    <option>Please select</option>\r\n                    {categories &&\r\n                        categories.map((c, i) => (\r\n                            <option key={i} value={c._id}>\r\n                                {c.name}\r\n                            </option>\r\n                        ))}\r\n                </select>\r\n            </div>\r\n\r\n            <div className=\"form-group\">\r\n                <label className=\"text-muted\">Shipping</label>\r\n                <select onChange={handleChange('shipping')} className=\"form-control\">\r\n                    <option>Please select</option>\r\n                    <option value=\"0\">No</option>\r\n                    <option value=\"1\">Yes</option>\r\n                </select>\r\n            </div>\r\n\r\n            <div className=\"form-group\">\r\n                <label className=\"text-muted\">Quantity</label>\r\n                <input onChange={handleChange('quantity')} type=\"number\" className=\"form-control\" value={quantity} />\r\n            </div>\r\n\r\n            <button className=\"btn btn-outline-primary\">Create Product</button>\r\n        </form>\r\n    );\r\n\r\n    const showError = () => (\r\n        <div className=\"alert alert-danger\" style={{ display: error ? '' : 'none' }}>\r\n            {error}\r\n        </div>\r\n    );\r\n\r\n    const showSuccess = () => (\r\n        <div className=\"alert alert-info\" style={{ display: createdProduct ? '' : 'none' }}>\r\n            <h2>{`${createdProduct}`} is created!</h2>\r\n        </div>\r\n    );\r\n\r\n    const showLoading = () =>\r\n        loading && (\r\n            <div className=\"alert alert-success\">\r\n                <h2>Loading...</h2>\r\n            </div>\r\n        );\r\n\r\n    ret